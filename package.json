{
  "name": "vue-vgavro-utils",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "humps": "=>2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vgavro/vue-vgavro-utils.git"
  },
  "author": "Victor Gavro <vgavro@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vgavro/vue-vgavro-utils/issues"
  },
  "eslintConfig": {
    "root": true,
    "extends": "standard",
    "env": {
      "es6": true
    },
    "plugins": [
      "pug",
      "html",
      "promise",
      "node"
    ],
    "globals": {
      "conf": "true"
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "ArrayExpression": "first",
          "ObjectExpression": "first",
          "CallExpression": {"arguments": "first"},
          "FunctionExpression": {"parameters": "first"}
        }
      ],
      "arrow-parens": 0,
      "comma-dangle": 0,
      "max-len": [
        1,
        100
      ],
      "prefer-const": 1,
      "no-return-assign": 0,
      "standard/no-callback-literal": 0
    }
  },
  "homepage": "https://github.com/vgavro/vue-vgavro-utils#readme"
}
